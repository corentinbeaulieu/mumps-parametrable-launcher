{
    "verbose": true,
    "debug": true,
    "EXPERIMENT": {
        "objectives": [
            "performance"
        ]
    },
    "PARAMETERS": {
        "DESIGN_PARAMETERS": {
            "num_threads": {
                "Type": "int",
                "Values": [
                    1,
                    5
                ]
            },
            "num_proc": {
                "Type": "int",
                "Values": [
                    1,
                    8 
                ]
            },
            "par": {
                "Type": "int",
                "Values": [
                    0,
                    1
                ]
            },
            "icntl_13": {
                "Type": "int",
                "Values": [
                    -1,
                    1
                ]
            }
        },
        "KERNEL_INPUTS": {
            "nnz": {
                "Type": "int",
                "Values": [
                    19000000,
                    25000000
                ]
            },
            "symmetry": {
                "Type": "int",
                "Values": [
                    0,
                    1
                ]
            }
        }
    },
    "SAMPLING": {
        "sampling_verbose": true,
        "sampling_debug": true,
        "sampler": {
            "sampling_method": "lhs",
            "parameters": {
                "n_samples": 100
            }
        },
        "scripts": {
            "kernel": "run_mumps.sh"
        },
        "kernel_arguments_order": [
            "nnz",
            "symmetry",
            "num_proc",
            "num_threads",
            "par",
            "icntl_13"
        ]
    },
    "MODELING": {
        "modeling_verbose": true,
        "modeling_debug": true,
        "modeling_method": "lightgbm",
    },
    "OPTIMIZATION": {
        "optimization_verbose": true,
        "optimization_debug": false,
        "sampling": {
            "sampler": {
                "sampling_method": "grid"
            },
            "sample_count": {
                "nnz": 20,
                "symmetry": 2
            }
        },
        "optimization_method": "genetic",
        "optimization_parameters": {
            "evolution": {
                "pop_size": 80
            },
            "termination": {
                "time": "00:01:00"
            },
	    "selection_method": "mono"
        }
    },
    "CLUSTERING": {
        "clustering_verbose": true,
        "clustering_debug": false,
        "clustering_method": "decision_tree",
        "clustering_parameters": {
            "max_depth": 3
        }
    }
}

